<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" 
"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="c" class="beans.Car" scope="">
		<property name="carNames"> 
			<list>
				<value>Audi</value>
				<value>Ferrari</value>
				<value>Benz</value>
			</list>
		</property>
		<property name="engines"> 
			<list>
				<ref bean="e1"/>
				<ref bean="e2"/>
				<ref bean="e3"/>
			</list>
		</property>
	</bean>
</beans>